{
  "name": "npool",
  "version": "0.1.0",
  "author": "Ryan Clements",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true yarn build",
    "export": "next export",
    "start": "next start",
    "sb": "start-storybook -p 6006 -s public",
    "sb:build": "build-storybook -s public",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write",
    "format:all": "yarn format .",
    "lint:all": "yarn lint:js:all && yarn lint:styles:all",
    "lint:js": "eslint --fix",
    "lint:js:all": "yarn lint:js .",
    "lint:styles": "stylelint --fix",
    "lint:styles:all": "yarn lint:styles ./**/*.{scss,css}",
    "print-browser-support": "browserslist --config=\".browserslistrc\"",
    "test": "jest",
    "test:all": "yarn lint:all && yarn type-check && yarn test",
    "prepare": "husky install",
    "chromatic": "env-cmd -f .env.local --silent chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "color": "^4.0.1",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.3.1",
    "tailwindcss": "^2.2.17"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.1.2",
    "@storybook/addon-a11y": "^6.3.9",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.3.9",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/color": "^3.0.2",
    "@types/jest": "^27.0.1",
    "@types/jest-axe": "^3.5.3",
    "@types/jest-when": "^2.7.3",
    "@types/lodash": "^4.14.175",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.21",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.3.6",
    "babel-jest": "^27.2.0",
    "chromatic": "^6.0.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-testing-library": "^4.12.4",
    "husky": "^7.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "jest-axe": "^5.0.1",
    "jest-mock-extended": "^2.0.4",
    "jest-watch-typeahead": "^0.6.4",
    "jest-when": "^3.4.1",
    "lint-staged": "^11.1.2",
    "next-router-mock": "^0.5.1",
    "postcss": "^8.3.8",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.4.1",
    "sass": "^1.41.1",
    "sass-loader": "^12.1.0",
    "storybook-addon-next-router": "^3.0.8",
    "storybook-dark-mode": "^1.0.8",
    "style-loader": "^3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-declaration-strict-value": "^1.7.12",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4",
    "webpack": "^5.53.0"
  }
}
