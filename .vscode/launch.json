{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Debug chrome",
      "url": "http://localhost:3000"
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Debug next",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229,
      "preLaunchTask": "yarn dev"
    }
  ],
  "compounds": [
    {
      "name": "Debug next and chrome",
      "configurations": ["Debug chrome", "Debug next"]
    }
  ]
}
